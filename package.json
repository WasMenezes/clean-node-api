      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^5.0.0-beta.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "standard": "^16.0.1"
  },
  "dependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "validator": "^13.1.17"
  }
}
