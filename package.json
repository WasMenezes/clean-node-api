{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --silent --verbose",
    "test:staged": "jest --passWithNoTests --findRelatedTests --silent",
    "test:ci": "jest --coverage --noStackTrace"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged",
      "git add"
